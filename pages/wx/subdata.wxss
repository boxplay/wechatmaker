.container {
	background: url('https://img.someet.cc/phpuQHR37') repeat;
	color: white;
	background-size: 100%;
	z-index:0;
}
.title-image{
	position: absolute;
	top: -50rpx;
	background: none;
	z-index: 12;
	background: none;
	width: 90%;
	left: 5%;
}
.pageTotal{
	width: 40rpx;
	height: 20rpx;
	line-height: 20rpx;
	text-align: center;
	margin: 0 auto;
	margin-bottom: 40rpx;
	font-size: 38rpx;
}
.firstPage-whitebox{
	width: calc(100vw - 42rpx);
	padding: 0.1px;
	margin: 0 auto;
	margin-top: 110rpx;
	position: relative;
	background-color: white;
	border-radius: 20rpx;
	color: black;
	border:2rpx solid black;
}
.firstPage-whitebox-bottom-box{
	width: calc(100vw - 42rpx);
	padding: 0.1px;
	left:-1rpx;
	position: absolute;
	background-color: white;
	border-radius: 20rpx;
	color: black;
	border:2rpx solid black;
	bottom:-8rpx;
	height: 50rpx;
	z-index:-1;
}
.firstPage-qabox{
	width: calc(100vw - 86rpx);
	margin: 0 auto;
	min-height: 65vh;
	/* padding: 0.1rpx; */
	/* overflow:hidden; */
}
.firstPage-answers{
	box-sizing: border-box;
	width: 100%;
	margin-top:20rpx ;
	position: relative;
}
.firstPage-questions{
	font-size: 39rpx;
	margin: 0 auto;
	margin-top: 48rpx;
	width: calc(100vw - 86rpx);
	text-align: left;
	font-weight: 500;
}
.product_type{
	display: inline-block;
	font-size: 28rpx;
	width: calc(100vw * .4);
	border:1rpx solid rgb(241,241,241);
	border-radius: 10rpx;
	color: rgb(45,45,45);
	background-color:rgb(241,241,241);
	margin-bottom: calc(100vw * .03);
	padding: 20rpx 0rpx;
	text-align: center;
}
.type_selected{
	background-color:#f5fcf4;
	border:1rpx solid #22dd03;
	color: #22dd03;
}
/* .product_type:nth-child(even){
	margin-left: calc(100vw * .1);
} */
.product_type:nth-child(odd){
	margin-right: calc(100vw * .03);
}
.line{
	width: calc(100vw - 86rpx);
	height: 2rpx;
	margin:0 auto;
	background-color: black;
	margin-top: 40rpx;
}
.firstPage-button-box{
	margin-top: 80rpx;
	margin-bottom: 80rpx;
}
