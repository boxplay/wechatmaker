.subdata-textarea{
	border:1rpx solid rgb(241,241,241);
	background-color: rgb(241,241,241);
	display: block;
	margin: 0 auto;
	width: auto;
	text-align: left;
	border-radius: 10rpx;
	padding: 24rpx;
	font-size: 28rpx;
	font-weight: 100;
}
.subdata-question-box{
	text-align: left;
	width: calc(100vw * .85);
	margin: 0 auto;
}
.subdata-question{
	/* padding: 15rpx 0 15rpx 0; */
	margin-top: 40rpx;
	margin-bottom: 20rpx;
	position: relative;
	font-size: 34rpx;
	font-weight: 500;

}
.subdata-input{
	margin-top: 0rpx;
}
.subdata-input input{
	background-color: rgb(241,241,241);
	border-radius: 10rpx;
	height: 45rpx;
	line-height: 45rpx;
	padding: 15rpx;
	padding-left: 55rpx;
	font-size: 28rpx;
	font-weight: 100;
}
.line-dashed{
	background-color: lightGray;
	height: 1px;
	width: calc(100vw - 86rpx);
	margin: 0 auto;
}
.subdata-button{
	border: none;
	text-align: center;
	width: calc(100vw - 86rpx);
	margin: 0 auto;
	color: #72e668;
	font-size: 30rpx;
	margin-top:30rpx;
	margin-bottom:30rpx;
	background-color: #f9f9f9;
}
.subdata-button::after{
	border:none;
}
.firstPage-button{
	width: calc(100vw - 42rpx);
	margin: 0 auto;
	height: 100rpx;
	line-height: 58rpx;
	border-radius: 100rpx;
	text-align: center;
	color:black;
	background-color: #ffce00;
	padding: 10rpx;
	position: relative;
	overflow: visible;
	z-index:20;
	font-weight: 700;
	border:1px solid black;
}
.firstPage-button::after{
	content: " ";
	margin: 0 auto;
	border-radius: 100rpx;
	text-align: center;
	color:black;
	background-color: #ffce00;
	padding: 10rpx;
	position: absolute;
	left:0;
	right: 0;
	top:-8rpx;
	z-index:-1;
	font-weight: 700;
	border:1px solid black;
}
.userinfoname{
	margin-top: 20rpx;
}
.userinfoname:after{
	background: url('data: image/png;base64,') no-repeat;
}
.userinfoid:after{
	background: url('data:image/png;base64,') no-repeat;
}
.userinfophone:after{
	background: url('data:image/png;base64,') no-repeat;
}
.subdata-input:after{
	background-size: 100%;
	width: 35rpx;
	height: 35rpx;
	z-index: 1;
	content: "";
	position: absolute;
	top: 25%;
	left: 10rpx;
}