.tab_bar{position:fixed;width:100vw;height:88rpx;background-color:#fff;z-index:9999;bottom:0;box-sizing:border-box;padding:0 0rpx;display:flex;flex-direction: colum;align-items: center;justify-content: space-between;box-shadow:0 -10rpx 20rpx rgba(0,0,0,.1);}

/* .tab_bar.iphonex{padding:50rpx 100rpx 0;} */
.tab_bar .icon_home,.tab_bar .icon_article,.tab_bar .icon_we{width:24%;height:88rpx;text-align:center;}
.tab_bar .icon_home .icon,.tab_bar .icon_article .icon,.tab_bar .icon_we .icon{height:88rpx;line-height:86rpx;}
.tab_bar .icon_home .icon.selected,.tab_bar .icon_article .icon.selected,.tab_bar .icon_we .icon.selected{height:120rpx;width:120rpx;background:#fff;border-radius:100%;box-shadow:0 -10rpx 20rpx rgba(0,0,0,.1);margin:-15rpx auto 0;line-height:88rpx;box-sizing:border-box;padding-top:15rpx;}
.tab_bar .icon_home .icon text,.tab_bar .icon_article .icon text,.tab_bar .icon_we .icon text{font-size:20rpx;display:block;color:#919191;line-height:20rpx;margin:-25rpx 0;padding:0;}

.tab_bar .icon_home .icon.icon.selected text,.tab_bar .icon_we .icon.icon.selected text,.tab_bar .icon_article .icon.selected text{color:#09bb07;margin:-30rpx 0;}

.tab_bar .icon_home .icon .home{
  margin:0;
  width:50rpx;
  height:50rpx;
  display:inline-block;
  background-image: url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.tab_bar .icon_article .icon .article{
  width:50rpx;
  height:50rpx;
  display:inline-block;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.tab_bar .icon_we .icon .we{
  width:50rpx;
  height:50rpx;
  display:inline-block;
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.tab_bar .icon_home .icon.selected .home{
  background-image:url(data:image/png;base64,);
}

.tab_bar .icon_article .icon.selected .article{background-image:url(data:image/png;base64,);}
.tab_bar .icon_we .icon.selected .we{
  background-image:url(data:image/png;base64,);
}
.top_bar{position:fixed;width:68rpx;height:68rpx;z-index:999999999;left:15rpx;top:15rpx;}
.top_bar .ctx{width:50rpx;height:50rpx;position:absolute;top:0;left:0;}
.top_bar .ctx.goback{
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.top_bar .ctx.menu{
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}
.top_bar .ctx.close{
  background-image:url(data:image/png;base64,);
  background-repeat:no-repeat;
  background-size:cover;
}

.menu_box{width:100vw;height:100%;position:fixed;left:-110vw;top:0;background-color:rgba(0,0,0,.2);z-index:99999;transition:all .5s ease-in-out;}
.menu_box scroll-view{background-color:#fff;width:calc(100vw * .8);padding:0;box-shadow:0 20rpx 20rpx rgba(0,0,0,.5);position:absolute;left:0;top:0;}
.menu_box .menu{width:calc(100vw * .8);height:100%;padding:64rpx 40rpx;box-sizing:border-box;}
.menu_box .menu .list{margin-bottom:100rpx;}

.menu_box .menu .list .main_title{font-size:34rpx;display:block;color:#010101;line-height:56rpx;margin:30rpx 0 30rpx 30rpx;}
.menu_box .menu .list .main_title.big{font-size:40rpx;margin:30rpx 0;}

.menu_box .menu .list .main_title .arrow{width:26rpx;height:16rpx;float:right;line-height:16rpx;margin-top:24rpx;display:inline-block;background:url(data:image/png;base64,) no-repeat;background-size:cover;}

.menu_box .menu .list .main_title.up .arrow{transform: rotate(180deg);line-height:56rpx;}
.menu_box .menu .list .main_title.up .list_cont{display:block;}

.menu_box .menu .list_cont{padding:20rpx 0 10rpx 30rpx;display:none;}
.menu_box .menu .list_cont text{display:block;font-size:28rpx;color:#434751;}

.menu_box .menu .list_cont.show{display:block;}
.menu_box .menu .list_cont.hide{display:none;}

.menu_box .menu .icons .main_title{display:block;text-align:center;font-size:36rpx;font-weight:500;margin-bottom:0;}
.menu_box .menu .icons .icon_list{clear:both;padding-bottom:50rpx;height:100%;}
.menu_box .menu .icons .icon_list .icon{float:left;width:calc(100% / 3);height:180rpx;text-align:center;}
.menu_box .menu .icons .icon_list .icon image{width:100%;height:100%;}
.menu_box .menu .icons .icon_list .icon text{display:block;text-align:center;font-size:24rpx;margin-top:-50rpx;}

.clearfix, .clear {zoom: 1;clear: both;}
.clearfix:before, .clearfix:after, .clear:before, .clear:after {content: " ";display: table;}
.clearfix:after, .clear:after {clear: both;}

